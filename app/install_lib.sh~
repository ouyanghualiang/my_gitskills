#!/bin/bash

adb push libs/armeabi/libatk.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libfontconfig.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libgdk.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libgmodule.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libgtk.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libcairo.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libfreetype.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libgio.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libgobject.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libpango.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libffi.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libgdk-pixbuf.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libglib.so /data/data/org.p2lang.gtkandroid/lib/
adb push libs/armeabi/libpcre.so /data/data/org.p2lang.gtkandroid/lib/
#adb push libs/armeabi/libharfbuzz.so /data/data/org.p2lang.gtkandroid/lib/

